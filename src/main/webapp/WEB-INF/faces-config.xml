<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
         <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pubCreate</from-outcome>
            <to-view-id>/publisherCreation.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>associatePub</from-outcome>
            <to-view-id>/bookPublisher.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>bookCreate</from-outcome>
            <to-view-id>/bookCreation.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>catCreate</from-outcome>
            <to-view-id>/categoryCreation.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>lookup</from-outcome>
            <to-view-id>/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/categories.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>associateCat</from-outcome>
            <to-view-id>/bookCategory.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>delete</from-outcome>
            <to-view-id>/bookDeletion.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/publisherCreation.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>display</from-outcome>
            <to-view-id>/publisher.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/bookPublisher.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>display</from-outcome>
            <to-view-id>/bookEdition.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/bookCreation.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>display</from-outcome>
            <to-view-id>/book.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/bookCategory.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>display</from-outcome>
            <to-view-id>/bookCategories.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
